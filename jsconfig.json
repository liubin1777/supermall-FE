{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@root/*": [
        "./src/*"
      ],
      "@asset/*": [
        "./src/assets/*"
      ],
      "@config/*": [
        "./src/config/*"
      ],
      "@page/*": [
        "./src/page/*"
      ],
      "@component/*": [
        "./src/components/*"
      ],
      "@service": [
        "src/service/index"
      ],
      "@service/*": [
        "src/service/*"
      ],
      "@util": [
        "src/util/index"
      ],
      "@util/*": [
        "src/util/*"
      ]
    }
  },
  "exclude": ["node_modules", "dist"]
}