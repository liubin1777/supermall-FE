{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@root/*": [
        "./src/*"
      ],
      "@page/*": [
        "./src/page/*"
      ],
      "@component/*": [
        "./src/components/*"
      ],
      "@service": [
        "src/service/index"
      ],
