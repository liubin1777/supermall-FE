    "classnames": "^2.3.1",
    "loadsh": "^0.0.4",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "swiper": "^8.1.5"
  },
  "scripts": {
    "server": "http-server ./server",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
